<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<mojos>
    <mojo>
      <goal>validate-code</goal>
      <description>Goal which validates the Java Quick Start project</description>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <threadSafe>false</threadSafe>
      <configuration>
      <parameters>
        <parameter>
            <name>
               <value lang="en">Validate Against</value>
            </name>
            <type>List</type>
            <required>false</required>
            <editable>true</editable>
            <description></description>
            <key>sonar</key>
            <possibleValues>
                <value key="src" dependency="skipTests">Source</value>
                <value key="functional">Functional Test</value>
            </possibleValues>
            <multiple>false</multiple>
            <sort>false</sort>
            <show>true</show>
          <value></value>
        </parameter>
        <parameter>
            <pluginParameter>plugin</pluginParameter>
            <mavenCommands>
                <mavenCommand>
                    <key>true</key>
                    <value>-DskipTests=true</value>
                </mavenCommand>
                <mavenCommand>
                    <key>false</key>
                    <value>-DskipTests=false</value>
                </mavenCommand>
            </mavenCommands>
            <name>
                <value lang="en">Skip Unit Test</value>
            </name>
            <type>Boolean</type>
            <required>false</required>
            <editable>true</editable>
            <description></description>
            <key>skipTests</key>
            <multiple>false</multiple>
            <sort>false</sort>
            <show>false</show>
            <value>false</value>
        </parameter>
    </parameters>
    </configuration>
    </mojo>
    <mojo>
        <goal>package</goal>
        <description>Goal which packages the Java-quickstart project</description>
        <requiresDirectInvocation>false</requiresDirectInvocation>
        <requiresProject>true</requiresProject>
        <requiresReports>false</requiresReports>
        <aggregator>false</aggregator>
        <requiresOnline>false</requiresOnline>
        <inheritedByDefault>true</inheritedByDefault>
        <implementation>
           <dependency>
               <groupId>com.photon.phresco.plugins</groupId>
               <artifactId>java-phresco-plugin</artifactId>
               <type>jar</type>
               <version>3.1.0.3001</version>
               <class>com.photon.phresco.plugins.java.JavaPlugin</class>
           </dependency>
       </implementation>
        <language>java</language>
        <instantiationStrategy>per-lookup</instantiationStrategy>
        <executionStrategy>once-per-session</executionStrategy>
        <threadSafe>false</threadSafe>
        <configuration>
            <parameters>
                <parameter>
                    <name>
                        <value lang="en">Build Name</value>
                    </name>
                    <type>String</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>buildName</key>
                    <multiple>false</multiple>
                    <sort>false</sort>
                    <show>true</show>
                    <value></value>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Build Number</value>
                    </name>
                    <type>Hidden</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>buildNumber</key>
                    <multiple>false</multiple>
                    <sort>false</sort>
                    <show>false</show>
                    <value>1</value>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Jar Name</value>
                    </name>
                    <type>String</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>jarName</key>
                    <multiple>false</multiple>
                    <value>HelloWorld</value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">MainClass Name</value>
                    </name>
                    <type>String</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>mainClassName</key>
                    <multiple>false</multiple>
                    <value>com.photon.phresco.HelloWorld</value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Environment</value>
                    </name>
                    <type>DynamicParameter</type>
                    <dynamicParameter>
                         <class>com.photon.phresco.impl.EnvironmentsParameterImpl</class>
                        <dependencies>
                            <dependency>
                                <groupId>com.photon.phresco.commons</groupId>
                                <artifactId>phresco-commons</artifactId>
                                <type>jar</type>
                                <version>3.1.0.3001</version>
                            </dependency>
                        </dependencies>
                    </dynamicParameter>
                    <required>true</required>
                    <editable>true</editable>
                    <key>environmentName</key>
                    <multiple>true</multiple>
                    <sort>false</sort>
                    <show>true</show>
                    <value>Production</value>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Show Settings</value>
                    </name>
                    <type>Boolean</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>showSettings</key>
                    <multiple>false</multiple>
                    <sort>false</sort>
                    <show>true</show>
                    <value>false</value>
                    <dependency>environmentName</dependency>
                </parameter>
                <parameter>
                    <pluginParameter>plugin</pluginParameter>
                    <mavenCommands>
                        <mavenCommand>
                            <key>true</key>
                            <value>-DskipTests=true</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>false</key>
                            <value>-DskipTests=false</value>
                        </mavenCommand>
                    </mavenCommands>
                    <name>
                        <value lang="en">Skip Unit Test</value>
                    </name>
                    <type>Boolean</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>skipTest</key>
                    <multiple>false</multiple>
                    <sort>false</sort>
                    <show>true</show>
                    <value>false</value>
                </parameter>
                <parameter>
                    <pluginParameter>framework</pluginParameter>
                    <mavenCommands>
                        <mavenCommand>
                            <key>showErrors</key>
                            <value>-e</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>hideLogs</key>
                            <value>-q</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>showDebug</key>
                            <value>-X</value>
                        </mavenCommand>
                    </mavenCommands>
                    <name>
                        <value lang="en">Logs</value>
                    </name>
                    <type>List</type>
                    <required>false</required>
                    <editable>true</editable>
                    <key>logs</key>
                    <possibleValues>
                        <value key="showErrors">Show Errors</value>
                        <value key="hideLogs">Hide Logs</value>
                        <value key="showDebug">Show Debug</value>
                    </possibleValues>
                    <multiple>false</multiple>
                    <sort>false</sort>
                    <show>true</show>
                    <value></value>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Package File Browse</value>
                    </name>
                    <type>packageFileBrowse</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>packageFileBrowse</key>
                    <multiple>false</multiple>
                    <value></value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
            </parameters>
        </configuration>
    </mojo>
    <mojo>
        <goal>functional-test-webdriver</goal>
        <description>Goal which functional Test-webdriver the Java Quick Start project</description>
        <requiresDirectInvocation>false</requiresDirectInvocation>
        <requiresProject>true</requiresProject>
        <requiresReports>false</requiresReports>
        <aggregator>false</aggregator>
        <requiresOnline>false</requiresOnline>
        <inheritedByDefault>true</inheritedByDefault>
        <language>java</language>
        <instantiationStrategy>per-lookup</instantiationStrategy>
        <executionStrategy>once-per-session</executionStrategy>
        <threadSafe>false</threadSafe>
        <configuration>
        <parameters>
            <parameter>
                <name>
                    <value lang="en">Test Against</value>
                </name>
                <type>List</type>
                <required>false</required>
                <editable>true</editable>
                <description></description>
                <key>testAgainst</key>
                <possibleValues>
                    <value key="jar" dependency="jarLocation">Jar</value>
				</possibleValues>
                <value></value>
                <sort>false</sort>
                <show>true</show>
            </parameter>
            <parameter>
                <name>
                    <value lang="en">JAR Location</value>
                </name>
                <type>FileBrowse</type>
                <required>false</required>
                <editable>true</editable>
                <description></description>
                <key>jarLocation</key>
                <multiple>false</multiple>
                <value></value>
                <sort>false</sort>
                <show>false</show>
                <fileType>jar</fileType>
            </parameter>
        </parameters>
    </configuration>
    </mojo>
    <mojo>
        <goal>pdf-report</goal>
        <description>Goal which generates pdf report for the project</description>
        <requiresDirectInvocation>false</requiresDirectInvocation>
        <requiresProject>true</requiresProject>
        <requiresReports>false</requiresReports>
        <aggregator>false</aggregator>
        <requiresOnline>false</requiresOnline>
        <inheritedByDefault>true</inheritedByDefault>
        <implementation>
            <dependency>
                <groupId>com.photon.phresco.plugins</groupId>
                <artifactId>report-phresco-plugin</artifactId>
                <type>jar</type>
                <version>3.1.0.3001</version>
                <class>com.photon.phresco.plugins.ReportPlugin</class>
            </dependency>
        </implementation>
        <language>java</language>
        <instantiationStrategy>per-lookup</instantiationStrategy>
        <executionStrategy>once-per-session</executionStrategy>
        <threadSafe>false</threadSafe>
        <configuration>
            <parameters>
                <parameter>
                    <name>
                        <value lang="en">Report Type</value>
                    </name>
                    <type>List</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>reportType</key>
                    <possibleValues>
                        <value key="crisp">Overall</value>
                        <value key="detail">Detailed</value>
                    </possibleValues>
                    <multiple>false</multiple>
                    <value></value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
				<parameter>
                    <name>
                        <value lang="en">Pdf Report Name</value>
                    </name>
                    <type>String</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>reportName</key>
                    <multiple>false</multiple>
                    <value></value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Test Type</value>
                    </name>
                    <type>List</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>testType</key>
                    <possibleValues>
                        <value key="All">All</value>
                        <value key="unit">unit</value>
                        <value key="functional">functional</value>
                        <value key="performance">performance</value>
                        <value key="load">load</value>
                    </possibleValues>
                    <multiple>false</multiple>
                    <value></value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Sonar Url</value>
                    </name>
                    <type>Hidden</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>sonarUrl</key>
                    <multiple>false</multiple>
                    <value></value>
                    <sort>false</sort>
                    <show>false</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Logo</value>
                    </name>
                    <type>Hidden</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>logo</key>
                    <multiple>false</multiple>
                    <value></value>
                    <sort>false</sort>
                    <show>false</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Theme</value>
                    </name>
                    <type>Hidden</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>theme</key>
                    <multiple>false</multiple>
                    <value></value>
                    <sort>false</sort>
                    <show>false</show>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Technology Name</value>
                    </name>
                    <type>Hidden</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>technologyName</key>
                    <multiple>false</multiple>
                    <value></value>
                    <sort>false</sort>
                    <show>false</show>
                </parameter>
            </parameters>
        </configuration>
    </mojo>
